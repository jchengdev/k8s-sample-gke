    "@types/react-copy-to-clipboard": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "babel-loader": "^8.2.4",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.0",
    "typescript": "4.6.2"
