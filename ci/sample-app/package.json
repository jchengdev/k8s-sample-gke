    "@mui/styles": "^5.5.1",
    "chroma-js": "^2.4.2",
    "classnames": "^2.3.1",
    "express": "^4.17.3",
    "next": "12.1.0",
    "rc-slider": "^10.0.0-alpha.6",
    "react": "17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@next/bundle-analyzer": "^12.1.0",
    "@storybook/addon-actions": "^6.4.19",
